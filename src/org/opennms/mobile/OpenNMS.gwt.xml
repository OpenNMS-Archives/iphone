<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.1.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.3.0-rc1/distro-source/core/src/gwt-module.dtd">
<module rename-to='OpenNMS'>
  <inherits name='com.google.gwt.user.User'/>

  <source path='client'/>
  <entry-point class='org.opennms.mobile.client.OpenNMS'/>

  <inherits name='com.gwtmobile.ui.gwtmobile_ui'/>
  <inherits name='com.google.gwt.logging.Logging'/>
  <!-- <inherits name="com.google.gwt.json.JSON" /> -->
  <!-- <inherits name='com.google.web.bindery.autobean.AutoBean'/> -->
  <!-- <inherits name='com.gwtmobile.phonegap.gwtmobile_phonegap' /> -->
  <inherits name="name.pehl.piriti.xml.XML" />

  <!-- 
  <inherits name="com.google.code.gwt.database.Html5Database" />
  <inherits name="com.google.code.gwt.storage.Html5Storage" />
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.activity.Activity'/>
  <inherits name="de.kurka.gwt.mobile.MobileGwt"/>
  <inherits name="de.kurka.gwt.mobile.theme.iphone.IphoneTheme"/>
  <inherits name="de.kurka.gwt.mobile.theme.ipad.IpadTheme"/>
  <inherits name="de.kurka.gwt.mobile.theme.android.AndroidTheme"/>
  -->
  
  <set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true" />
  <set-property name="user.agent" value="safari" />

  <set-property name="gwt.logging.consoleHandler" value="DISABLED" />
  <set-property name="gwt.logging.developmentModeHandler" value="ENABLED" />
  <set-property name="gwt.logging.firebugHandler" value="DISABLED" />
  <set-property name="gwt.logging.popupHandler" value="DISABLED" />
  <set-property name="gwt.logging.systemHandler" value="DISABLED" />
  <set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED" />


</module>
